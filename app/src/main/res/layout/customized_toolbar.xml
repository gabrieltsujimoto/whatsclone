<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/tbMain"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/Hellowmotow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hellow motow"
        app:layout_constraintBottom_toBottomOf="@+id/tbMain"
        app:layout_constraintEnd_toEndOf="@+id/tbMain"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="@+id/tbMain"
        app:layout_constraintTop_toTopOf="@+id/tbMain" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#E5FCF5"
        app:layout_constraintBottom_toTopOf="@+id/tbMain"
        app:layout_constraintEnd_toEndOf="@+id/tbMain"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toEndOf="@+id/Hellowmotow"
        app:layout_constraintTop_toBottomOf="@+id/tbMain"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/search_icon" />
</androidx.constraintlayout.widget.ConstraintLayout>